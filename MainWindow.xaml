<Window x:Class="LoginRegistroUsuarios.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LoginRegistroUsuarios"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Menu Height="18">
                <MenuItem Header="Registros">
                    <MenuItem x:Name="UsuariosMenuItem" Header="Registro usuarios" Click="UsuariosMenuItem_Click" />
                </MenuItem>

                <MenuItem Header="Consultas">
                    <MenuItem x:Name="ConsultaUsuarioItem" Header="Consulta de usuarios" Click="ConsultaUsuarioItem_Click"/>
                </MenuItem>

            </Menu>

        </StackPanel>
    </Grid>
</Window>
